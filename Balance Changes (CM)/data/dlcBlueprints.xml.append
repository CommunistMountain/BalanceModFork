        <mod:findName type="augBlueprint" name="TELEPORT_HEAL"/>
        <mod:findName type="augBlueprint" name="BATTERY_BOOSTER"/>
    </mod:par>
    <mod:findLike type="rarity">
        <mod:setValue>0</mod:setValue>
    </mod:findLike>
</mod:findComposite>

<mod:findName type="augBlueprint" name="FLEET_DISTRACTION">
    <mod:findLike type="cost">
