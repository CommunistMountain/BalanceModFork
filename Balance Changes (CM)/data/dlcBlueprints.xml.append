        <mod:findName type="shipBlueprint" name="PLAYER_SHIP_ANAEROBIC"/>
        <mod:findName type="shipBlueprint" name="PLAYER_SHIP_ANAEROBIC_2"/>
    </mod:par>
    <mod:findLike type="aug">
        <mod:removeTag/>
    </mod:findLike>
</mod:findComposite>

<mod:findName type="shipBlueprint" name="PLAYER_SHIP_ANAEROBIC">
    <mod:findLike type="systemList">
        <mod:findLike type="pilot">
            <mod:setAttributes power="2"/>
        </mod:findLike>
    </mod:findLike>
    <mod:findLike type="weaponList">
        <mod:setAttributes missiles="0"/>
    </mod:findLike>
    <mod:findLike type="droneList">
        <mod:setAttributes drones="20"/>
    </mod:findLike>
    <mod:findLike type="maxPower">
        <mod:setAttributes amount="7"/>
    </mod:findLike>
    <mod:findLike type="crewCount">
        <mod:removeTag/>
    </mod:findLike>
    <mod-append:crewCount amount="1" class="anaerobic"/>
</mod:findName>

<mod:findName type="shipBlueprint" name="PLAYER_SHIP_ANAEROBIC_2">
    <mod:findLike type="systemList">
        <mod:findLike type="sensors">
            <mod:setAttributes start="true"/>
        </mod:findLike>
        <mod:findLike type="clonebay">
            <mod:setAttributes start="false"/>
        </mod:findLike>
        <mod:findLike type="teleporter">
            <mod:setAttributes power="3"/>
        </mod:findLike>
    </mod:findLike>
    <mod:findLike type="maxPower">
        <mod:setAttributes amount="9"/>
    </mod:findLike>
</mod:findName>

